# Amaya AI Assistant 环境变量配置示例
# 将此文件复制为 .env 并填入真实的配置值

# OneBot V11 配置
ONEBOT_WS_URL=ws://127.0.0.1:3081
ONEBOT_ACCESS_TOKEN=your_access_token_here

# 用户配置
DEFAULT_USER_ID=user_default

# Gemini API 配置
GEMINI_API_ENDPOINT=yunwu.ai
GEMINI_MODEL_ID=gemini-2.5-pro-preview-06-05
GEMINI_API_KEY=your_secret_api_key_here

# 记忆配置
SHORT_TERM_MEMORY_TOKEN_LIMIT=2000

# 日志配置（可选）
LOG_LEVEL=INFO
LOG_FILE=amaya_log.jsonl